#include<stdio.h>
int main()
{
    int i,j,n;
    scanf("%d",&n);
    for(i= 1;i<=n;i++){
        int count=0;
         for(j=2;j<=i/2;j++){
             if(i%j==0){
                 count++;
                 break;
             }
        }
         if(count==0)
             printf("%d ",i);
    }
   return 0;
}
